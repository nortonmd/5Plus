<aura:component description="HelloWorld" implements="flexipage:availableForAllPageTypes,force:appHostable" access="global">
    <aura:attribute name="whom" type="String" access="global" default="World" />
    <h1 class="title">Hello, {!v.whom}!</h1>
    <ui:inputSelect class="single" aura:id="InputSelectSingle" change="{!c.onSingleSelectChange}">
        <ui:inputSelectOption text="Johnny Tow Chairs" />
        <ui:inputSelectOption text="Jeffrey Shu" />
        <ui:inputSelectOption text="No QEA" />
    </ui:inputSelect>
</aura:component>
